<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cykj.domeboot.mapper.UserMapper">
    <select id="login" resultType="com.cykj.domeboot.entity.TbUser" parameterType="com.cykj.domeboot.entity.TbUser">
        select * from  tb_user where account=#{account} and pwd=#{pwd}
    </select>

</mapper>